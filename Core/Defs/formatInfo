# Dynamic library format   information.

# fields are:
# module pre|post   string  lib

display  filePrefix @X      RavlDPDisplay
csp      filePrefix @CSP    CSPDriver
DIVO     filePrefix @DIVO   RavlDMedia
net      filePrefix @NET    RavlNet
v4l	 filePrefix @V4L    RavlImgIOV4L
devaudio filePrefix @DEVAUDIO RavlDevAudio
meteor1  filePrefix @METEOR RavlImgIOMeteor1
graph	 filePrefix @GRAPH  RavlDPGraph
curl	 filePrefix @http   RavlURLIO
curl	 filePrefix @https  RavlURLIO
curl	 filePrefix @ftp    RavlURLIO
curl	 filePrefix @ldap   RavlURLIO
jpeg	 fileExt    jpg     RavlExtImgIO
png	 fileExt    png     RavlExtImgIO
cif	 fileExt    cif	    RavlVideoIO
cif	 fileExt    qcif    RavlVideoIO
cif	 fileExt    sqcif   RavlVideoIO
yuv	 fileExt    yuv     RavlVideoIO
syuv	 fileExt    syuv    RavlVideoIO
avi	 fileExt    avi     RavlVideoIO
dv	 fileExt    dv      RavlDV
pgm	 fileExt    pgm     RavlImageIO
ppm	 fileExt    ppm     RavlImageIO
pbm	 fileExt    pbm     RavlImageIO
rgb	 fileExt    rgb     RavlImageIO
tri	 fileExt    tri     Ravl3DIO
obj	 fileExt    obj	    Ravl3DIO
wrl	 fileExt    wrl	    Ravl3DIO
libaudio fileExt    wav	    RavlAudioFile
libaudio fileExt    aiff    RavlAudioFile
libaudio fileExt    aiffc   RavlAudioFile
libaudio fileExt    au      RavlAudioFile
libaudio fileExt    nextsnd RavlAudioFile
libaudio fileExt    bicsf   RavlAudioFile
mpeg2	 fileExt    mpeg    RavlMPEG2

# DataSets

abs	 symb	    RavlN::DataSet2C<SampleVectorC,SampleLabelC> RavlPatternRecIO

# Samples

txt	 symb	    RavlN::SampleC<VectorC>			RavlPatternRecIO
abs	 symb	    RavlN::SampleC<UIntT>			RavlPatternRecIO
abs	 symb	    RavlN::SampleC<VectorC>			RavlPatternRecIO
abs	 symb	    RavlN::SampleVectorC			RavlPatternRecIO
abs	 symb	    RavlN::SampleLabelC				RavlPatternRecIO
abs	 symb	    RavlN::SArray1dC<VectorC>			RavlPatternRecIO

# Classifiers.

abs	 symb	    RavlN::ClassifierC				RavlPatternRecIO
abs	 symb	    RavlN::ClassifierNearestNeighbourC		RavlPatternRecIO
abs	 symb	    RavlN::ClassifierKNearestNeighbourC		RavlPatternRecIO
abs	 symb	    RavlN::ClassifierAverageNearestNeighbourC	RavlPatternRecIO
abs	 symb	    RavlN::ClassifierDiscriminantFunctionC	RavlPatternRecIO
abs	 symb	    RavlN::ClassifierGaussianMixtureC		RavlPatternRecIO
abs	 symb	    RavlN::ClassifierFunc1ThresholdC		RavlPatternRecIO
abs	 symb	    RavlN::ClassifierWeakLinearC		RavlPatternRecIO
abs	 symb	    RavlN::ClassifierLinearCombinationC		RavlPatternRecIO

# Functions

abs	 symb	    RavlN::FunctionC				RavlPatternRecIO
abs	 symb	    RavlN::Function1C				RavlPatternRecIO
abs	 symb	    RavlN::FuncMeanProjectionC			RavlPatternRecIO
abs	 symb	    RavlN::FuncLinearC				RavlPatternRecIO
abs	 symb	    RavlN::FuncQuadraticC			RavlPatternRecIO
abs	 symb	    RavlN::FuncOrthPolynomialC			RavlPatternRecIO
abs	 symb	    RavlN::FuncLinearCoeffC			RavlPatternRecIO
abs	 symb	    RavlN::FuncPDFNormalC			RavlPatternRecIO
abs	 symb	    RavlN::GaussianMixtureC			RavlPatternRecIO

# Distances

abs	 symb	    RavlN::DistanceC				RavlPatternRecIO
